                    <div class="scrollbar" id="scroller">
                        <div class="row" ng-if="currentView === 'step_one'" ng-cloak>
                            <h5 class="model-title">Step 1 : Choose the Channels</h5>
                            <div class="col-lg-12">
                                <div id="error"></div>
                                <div ng-repeat="data in channelList | filter:{integrated: true}" name="{{data._id}}"
                                     class="col-lg-2 widget-icons"
                                     ng-click="storeChannel(data);"
                                     id="channellist-{{data._id}}"
                                     ng-class="{'channelSelect': data.isSelected==1}">
                                        <img ng-hide="data.name==='CustomData'" src="image/lightbox/{{data.name}}.png"
                                         style="max-width: 100%;max-height:100%;padding: 15px 5px; "/>
                                        <p ng-show="data.name==='CustomData'" class="model-title"
                                           style="padding: 25px 5px;">Custom Data</p>
                                </div>
                                <div><!--style="float:left"--><!--<h4>Coming soon</h4>-->
                                    <div ng-repeat="data in channelList | filter:{integrated: false}"
                                         class="col-lg-2 widget-icons" id="soonModel" ng-click="ComingSoonAlert()">
                                        <img src="image/lightbox/{{data.name}}.png"
                                             style="max-width: 100%;max-height:100%;padding: 15px 5px; "/>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row" ng-if="currentView === 'step_two'" ng-cloak>
                            <h5 class="model-title">{{selectCustomLinkHead}}</h5>
                            <div class="col-lg-12">
                                <div id="error"></div>
                                <div class="col-lg-12" ng-repeat="referenceWidgetsList in referenceWidgetsList">
                                    <div class="col-xs-12"><img src="image/lightbox/{{referenceWidgetsList.channelName}}.png" style="width:150px;" ng-class="{'mozImage': referenceWidgetsList.channelName=='Moz'}"/></div>
                                    <div ng-repeat="referenceWidgets in referenceWidgetsList.widgets"
                                         name="{{referenceWidgets.name}}" ref="{{referenceWidgets.isSelectedMetric}}"
                                         id="referenceWidgets-{{referenceWidgets._id}}"
                                         style="border:{{referenceWidgets.border}}" ng-hide="metricContent"
                                         class="col-lg-2 widget-icons showReferenceWidgets"
                                         ng-click="storeReferenceWidget();">
                                        <div ng-if="referenceWidgets.isSelectedMetric==1">
                                            <div id="triangle-topright-{{referenceWidgets._id}}" class="triangle-topright">
                                                <i id="getCheck-{{referenceWidgets._id}}" class="fa fa-check getCheck"
                                                   style="display:block;"></i>
                                            </div>

                                            <p id="metricNames-{{referenceWidgets._id}}" class="metricNames getMetricName">
                                                {{referenceWidgets.name}}</p>
                                        </div>

                                        <div ng-if="!(referenceWidgets.isSelectedMetric==1)">
                                            <div id="triangle-topright-{{referenceWidgets._id}}">
                                                <i id="getCheck-{{referenceWidgets._id}}" class="fa fa-check getCheck"></i>
                                            </div>

                                            <p id="metricNames-{{referenceWidgets._id}}" class="metricNames">
                                                {{referenceWidgets.name}}</p>
                                        </div>
                                        <!-- <P class="metricDescription" >{{referenceWidgets.description}}</P> -->
                                    </div>
                                </div>

                                <div class="col-lg-11 showCustomUrlLink well" ng-hide="showCustomContent">
                                    <h4 ng-hide="customMessage">
                                        Please send data through API to this URL - <a class="customApiLink"
                                                                                      ng-model="customWidgetId"> </a>
                                        <button class="btn" id="btnCopyLink" data-clipboard-text={{customLink}}
                                                ng-click="copyToClipboard()">
                                            <img src="image/clippy.svg" width="13" alt="Copy to clipboard">
                                        </button>
                                    </h4>
                                    <br/>
                                    <p><h4 ng-hide="customDocLinkMessage"><a href="/customDataDocumentation"
                                                                             target="_blank" class="findOutHow">Find out
