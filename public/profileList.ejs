<div class="modal-dialog modal-lg" ng-init="open()">
    <script type="text/ng-template" id="modal.ejs">
        <div class="modal-content" class="profilelistmodal" ng-controller="ProfileListController" ng-init="configProfileListModal()">
        <div class="modal-header">
            <button type="button" class="close"
                    ng-click="closeProfilelistModal()"><span
                        aria-hidden="true">Ã—</span><span class="sr-only">Close</span></button>
            <h3 class="modal-title">
                Linked Profiles
            </h3>
        </div>
        <div class="modal-body" >
            <div class="scrollbar">
                <div id ="errorInRelink"></div>
                <div class="row" class="profiletitle" style="height:40px;margin-top: 15px;">
                    <div class="col-sm-4">Channel</div>
                    <div class="col-sm-4">profileName</div>
                </div>
                <div class="row" ng-repeat="profile in profileListArray" style="height:40px">
                    <div class="col-sm-4"> <img  src="image/thumbnail/{{profile.channelName}}.png" class="img-thumbnail" style="max-width:10%;max-height:10%; margin: 0px 5px 0px 0px;"/></div>
                    <div class="col-sm-4">{{profile.name}}</div>
                    <div ng-show="profile.hasNoAccess" class="col-sm-4" ><a  style="color: #45596b" ng-click="refreshProfile(profile)" ><i class="fa fa-refresh" aria-hidden="true"></i></a></div>
                </div>
            </div>

        </div>
            </div>
            </script>
    </div>