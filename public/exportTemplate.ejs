<div id="exportModel" ng-init="openPDFModal('pdfExport')"  style="width:1100px!important;">
    <script src="js/datePicker/Calendar.js"></script>
    <script type="text/ng-template" id="modal.ejs">
        <div class=" modal-expContent" ng-controller="ExportController as expCtrl" ng-cloak>
            <button type="button" class="overlayCloseButton" ng-click="closeExportModal()" ng-if="closedCancle==true">
                <i class="fa fa-times fa-lg" aria-hidden="true"></i>
            </button>
            <div class="row" style="margin-left: 0px;margin-right:0px;padding-top: 15px">
                <div class="col-lg-12" ng-show="showExport == true">
                    <h5 class="model-title">Please Choose your Export option</h5>
                    <div class="wrapper wrapper-content">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="center">
                                    <label class="btn btn-default"><input type="radio" id="exportOptionJpeg" name="exportOption" value="JPEG" ng-click="pdfPrintPreview(false);"/> JPEG</label>
                                    <label class="btn btn-default"><input type="radio" id="exportOptionPDF" name="exportOption" value="PDF" ng-click="fetchLogosFromDB('org');fetchLogosFromDB('cli');pdfPrintPreview(true);printpaperPDF();"/> PDF</label>
                                    <label class="btn btn-default"><input type="radio" id="exportOptionUrl" name="exportOption" value="PDF" ng-click="pdfPrintPreview(false);"/> Share URL</label>
                                    <label class="btn btn-default"><input type="radio" id="exportOptionExcel" name="exportOption" value="Excel" ng-click="pdfPrintPreview(false);exportExcel()"/> Excel</label>
                                </div>
                            </div>
                        </div>
                        <div class="row" style="padding-left: 20px;padding-top: 25px;margin-left: 0px;margin-right:0px"  ng-if="expAct==true && exportPDF==true">
                            <div class="col-lg-12">
                                <div class="col-lg-6" style="border-right:1px dotted #aeaeae">
                                    <legend style="text-align: center;font-family: Montserrat-Black;font-size:18px;color:#1074ae !important">Choose Your Logo</legend>
                                    <div class="scrollbar" id="scroller" style="height:80px!important;">
                                        <div class="col-lg-2 logo-icons orgIcon" id="orgLogoIcons-default">
                                            <div style="height:15%;float:right;margin: -5px 0px 0px 0px;"></div>
                                            <img src="/userFiles/datapoolt.png" ng-click="selectedIconIndicator('org','orgLogoIcons-default');selectedOrgLogo('/userFiles/datapoolt.png')" style="max-width: 100%;height:85%;padding: 5px 5px; "/>
                                        </div>
                                        <div class="col-lg-2 logo-icons orgIcon" ng-repeat="data in orgLogosList" id="orgLogoIcons-{{data._id}}">
                                            <div  style="height:15%;float:right;margin: -5px 0px 0px 0px;">
                                                <i id="getCheck-{{data._id}}" class="fa fa-minus-square" style="display:block;" ng-click="removeLogo(data,'org')"></i>
                                            </div>
                                            <img ng-src="{{data.fileUrl}}"style="max-width: 100%;height:85%;padding: 5px 5px;" ng-click="selectedIconIndicator('org','orgLogoIcons-'+data._id);selectedOrgLogo(data.fileUrl)"/>
                                        </div>
                                    </div>
                                    <form ref='uploadOrgLogoForm' id='uploadOrgLogoForm' name="uploadOrgLogoForm"  style="padding: 5px 5px;">
                                        <div class="col-sm-6"><input type="file" name="sampleFile" file-model="orgFile" ng-click="enableUploadbutton('org')" required/></div>
                                        <div class="col-sm-6"><button type="submit" style="padding:9px 19px" class="componentBtn btn-4 btn-4a" id='orgUploadButton' ng-click="submitUpload(orgFile,'org')" disabled="disabled">Upload</button></div>
                                    </form>
                                </div>
                                <div class="col-lg-6">
                                    <legend style="text-align: center;font-family: Montserrat-Black;font-size:18px;color:#1074ae !important">Choose Client Logo (Optional)</legend>
                                    <div class="scrollbar" id="scroller" style="height:80px!important">
                                        <div class="col-lg-2 logo-icons cliIcon"  id='cliLogoIcons-default'>
                                            <div style="height:15%;float:right;margin: -5px 0px 0px 0px;"></div>
                                            <img src='/userFiles/plain-white.jpg' ng-click="selectedIconIndicator('cli','cliLogoIcons-default');selectedCliLogo('/userFiles/plain-white.jpg')" style="max-width: 100%;height:85%;padding: 5px 5px; "/>
                                        </div>
                                        <div class="col-lg-2 logo-icons cliIcon" ng-repeat="data in cliLogosList" id="cliLogoIcons-{{data._id}}">
                                            <div style="height:15%;float:right;margin: -5px 0px 0px 0px;">
                                                <i id="getCheck-{{data._id}}" class="fa fa-minus-square" style="display:block;"  ng-click="removeLogo(data,'cli')"></i></div>
                                            <img ng-src="{{data.fileUrl}}" ng-click="selectedIconIndicator('cli','cliLogoIcons-'+data._id);selectedCliLogo(data.fileUrl)" style="max-width: 100%;height:85%;padding: 5px 5px;"/>
                                        </div>
                                    </div>
                                    <form ref='uploadClientLogo' id='uploadClientLogo'>
                                        <div class="col-sm-6"><input type="file" name="sampleFile" file-model="cliFile" ng-click="enableUploadbutton('cli')"/></div>
                                        <div class="col-sm-6"><button type="submit" style="padding:9px 19px" class="componentBtn btn-4 btn-4a" id="clientUploadButton" ng-click="submitUpload(cliFile,'cli')" disabled="disabled">Upload</button></div>
                                    </form>
                                </div>
                            </div>
                        </div>
                        <div class="row" style="margin-left: 0px;margin-right:0px">
                            <div class="col-lg-12" text-center>
                                <button class="ladda-button componentBtn btn-4 btn-4a icon-arrow-right" data-style="expand-right" id="submitExportButton">
                                    <span class="ladda-label">Export</span><span class="ladda-spinner"></span><div class="ladda-progress" style="width: 72px;"></div>
                                </button>
                                <!-- <button class="btn btn-primary button-mode" id="submitExportButton" style="float:right;border: none;font-size: 17px;font-weight: 500;border-radius: 5px;margin: 0px 15px 15px 15px;cursor: pointer;" ng-click="closeExport();">Export</button>-->
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-12" ng-if='showExport == false' ng-show="showExport == false" ng-cloak>
                    <div class="md-content previewContentText">
                        <div>
                            <div>
                                <div class="row preview-loading" style="margin-left: 0px;margin-right:0px">
                                    <div class="col-md-12">
                                        <div class="center">
                                            <div class="mainLoading location">
                                                <div class="sk-spinner sk-spinner-double-bounce">
                                                    <div class="sk-double-bounce1"></div>
                                                    <div class="sk-double-bounce2"></div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br/>
                                <div class="row" style="margin-left: 0px;margin-right:0px">
                                    <div class="col-md-12">
                                        <h2 class="exportContentText">Initializing Export Please wait for a moment</h2>
                                    </div>
                                </div>
                                <br/>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="modalExp-body" ng-if='expAct==true' ng-show="expAct==true">
                <div class="wrapper wrapper-content" ng-show="exportUrl===true">
                    <div class="row" ng-cloak>
                        <div class="col-lg-12">
                            <div class="center shareUrl">
                                <p id="butt" style="word-wrap: break-word;">URL to be shared: <a style="color:#1078ae" href="{{'https://'+sharingUrl}}" target="_blank">{{sharingUrl}}</a>
                                    <button class="btn" id="btnCopyLink" data-clipboard-text={{'https://'+sharingUrl}}>
                                        <img src="image/clippy.svg" width="13" alt="Copy to clipboard">
                                    </button>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="wrapper wrapper-content" ng-if="exportPDF===true">
                    <h5 class="model-title">Print Preview</h5>
                    <div class="scrollbar" id="scroller">
                        <div class="row" style="margin-left: 0px;margin-right:0px;margin-bottom: 60px;">
                            <div class="col-lg-12"style="padiding:0px 0px !important">
                                <div id="dashLayout" class="dashboardLayout" style="border:1px solid #efefef">
                                    <div class="white-bg"style="border:0px solid #efefef" ng-repeat="(aexpageIndex,aexppage) in expPages">
                                        <div id="dashLayoutpages-{{aexpageIndex}}">
                                            <div class="row" style="border-bottom:1px solid #efefef;max-height:40px;margin-left: 0px;margin-right:0px">
                                                <div class="col col-sm-12">
                                                    <div class="expdashboardTemplate-items">
                                                        <div class="col-sm-2" style="float: left;"><img ng-src="{{orgLogoSrc}}" style="max-width: 90%;max-height:35px;padding: 0px 5px;"/></div>
                                                        <div class="col-sm-4 dashboardName" style="float:left;"> <pre ng-style="!expCtrl.dashboard.dashboardName  && {'color':'#999'}  || expCtrl.dashboard.dashboardName && {'color':'#000'}">{{expCtrl.dashboard.dashboardName || 'Name your dashboard'}}</pre></div>
                                                        <div class="col-sm-4"style="float: left;"><div id='expdaterange'class="daterange daterange--double three"></div></div>
                                                        <div class="col-sm-2"style="float: right;"><img ng-src="{{cliLogoSrc}}" style="max-width: 90%;max-height:35px;padding: 0px 5px;float:right;"/></div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div gridster="expgridsterOptions" class="row" id="expgridsterItems" style="margin:0px;"><!--ng-switch-when="true"-->
                                                <ul>
                                                    <li gridster-item="widget" ng-repeat="(widgetIndex,widget) in aexppage.widgets">
                                                        <div class="box getBox-{{widget.id}}" ng-controller="CustomWidgetController">
                                                            <div class="box-header" id="getWidgetColor-{{widget.id}}" ref="{{aexppage.widgetData[widgetIndex].color}}" style="background-color: #ffffff;"> <!--ng-style="{'background-color': aexppage.widgetData[widgetIndex].color}"-->
                                                                <div style="float: left;width: 93%;">
                                                                    <img ng-src="image/thumbnail/{{widget.channelName}}.png" ng-if="widget.channelName=='custom'||widget.channelName=='customFusion'"  style="vertical-align: super;max-height: 31px; max-width: 50px; padding:5px"/>
                                                                    <h3 ng-style="widget.sizeX!=1  && {'width':'80%'}  || widget.sizeX==1 && {'width':'50%'}" style="float:left;padding:7px!important;text-overflow:ellipsis;">{{aexppage.widgetData[widgetIndex].name}}</h3>
                                                                </div>
                                                            </div>
                                                            <div class="box-content" style="vertical-align: middle" ng-style="{position:(isMobile?'static':'absolute')}">
                                                                <div ng-if="aexppage.widgetData[widgetIndex].visibility == false">
                                                                    <div  ng-if="aexppage.widgetData[widgetIndex].dataerror == true" >
                                                                        <div class="row" style=" margin-top: 18%;height: 100%">
                                                                            <div style="height:99%;">
                                                                                <div style="position:relative; top:25%; text-align:center; padding-right:20px">
                                                                                    <p style="word-wrap:break-word;font-size: 10px;">Access to one or more profiles has expired</p>
                                                                                    <p><i style="color: #23527c;" class="fa fa-refresh" aria-hidden="true"> </i><p style="color:#000;font-size: 10px">  Refresh</p></p>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div  ng-if="aexppage.widgetData[widgetIndex].dataerror == false" >
                                                                        <div class="row" style="margin-top: 18%;height: 100%">
                                                                            <div style="height:99%;">
                                                                                <div style="position:relative; top:36%; text-align:center; padding-right:20px">
                                                                                    <p style="word-wrap:break-word;font-size: 12px;">Your credential is insufficient to fetch data for this metric</p>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="row getWidgetGraph" style="height: 100%;margin:0px;" ng-if="aexppage.widgetData[widgetIndex].visibility == true">
                                                                    <div ng-repeat="(chartIndex,chart) in aexppage.widgetData[widgetIndex].chart" ng-if="chart.options.chart.type=== 'lineChart' || chart.options.chart.type=== 'engagedUsersReach' || chart.options.chart.type=== 'area' ||chart.options.chart.type=== 'pieChart' || chart.options.chart.type=== 'reachVsImpressions' || chart.options.chart.type=== 'multiBarChart' || chart.options.chart.type=== 'multiChart'" style="float: left;padding-left: 2px;padding-right: 2px;" ng-style="{'height': (100/aexppage.widgetData[widgetIndex].layoutOptionsY)+'%', 'width': (100/aexppage.widgetData[widgetIndex].layoutOptionsX)+'%'}">
                                                                        <div class="col col-lg-4 col-sm-4 col-md-4" ng-if="aexppage.widgetData[widgetIndex].showSummary !=false" style="margin:0px;height:100%;padding-left:5px;padding-right:5px;"><!--  ng-style="calculateSummaryHeight(widget.sizeY,chart.data.length)">-->
                                                                            <ul style="height:100%;font-size:12px;padding-left: 5px;">
                                                                                <li  ng-repeat="data in chart.data" ng-class="{'summaryAlign': chart.data.length<=3}" style="text-align: center;padding-top: 1px;padding-left:0px;padding-right: 2px;font-size: 1.0em!important;" id="li-{{widget.id}}-{{$index}}" class="col col-lg-12" ng-style="calculateRowHeight(data,chart.data.length,widget.sizeY,aexppage.widgetData[widgetIndex].layoutOptionsX)"><!--size-watcher-width="data.mywidth"--><!--ng-class="calculateColumnWidth(chart.data.length,widget.sizeX,aexppage.widgetData[widgetIndex].layoutOptionsX)" ng-style="calculateRowHeight(data,chart.data.length,widget.sizeX,widget.sizeY,aexppage.widgetData[widgetIndex].layoutOptionsX)"-->
                                                                                    <div style="margin: 0px;height: 100%;overflow: hidden;white-space: nowrap;color: #232c3b;text-align: center;" >
                                                                                        <p class="summaryKeyFont" ng-class="{'reportSummaryKeyDoubleFont': customReport.widgetData[widget.id].layoutOptionsX>1}" ng-if="chart.options.chart.type != 'pieChart'" ng-show="data.showComparision==false" ng-style="{ 'font-family' : 'Montserrat-Regular','text-transform':'uppercase',  'border-bottom': '4px solid', 'border-color': data.color, 'padding-left': '3px'}" style="margin:0px 0px 3px!important;font-weight: bold;padding-top: 2px;padding-bottom: 2px;"> {{data.key}} - {{data.summaryDisplay}} <i ng-if="data.arrow==='up' && data.variance !=0" style="font-size:1.0em;color:#2A8539" class="fa fa-arrow-up" aria-hidden="true"></i><i ng-if='data.arrow==="down" && data.variance !=0' style="font-size: 1.0em;color:#FF1B1B" class="fa fa-arrow-down" aria-hidden="true"></i></p>
                                                                                        <p class="summaryKeyFont" ng-class="{'reportSummaryKeyDoubleFont': customReport.widgetData[widget.id].layoutOptionsX>1}" ng-if="chart.options.chart.type == 'pieChart'" ng-show="data.showComparision==false" ng-style="{ 'font-family' : 'Montserrat-Regular','text-transform':'uppercase',  'border-bottom': '4px solid', 'border-color': data.color, 'padding-left': '3px'}" style="margin:0px 0px 3px!important;font-weight: bold;padding-top: 2px;padding-bottom: 2px;"> {{data.key}} - {{data.summaryDisplay}}</p>
                                                                                        <p class="summaryKeyFont" ng-class="{'reportSummaryKeyDoubleFont': customReport.widgetData[widget.id].layoutOptionsX>1}" ng-show="data.showComparision==true" ng-style="{ 'font-family' : 'Montserrat-Black','text-transform':'uppercase',  'border-bottom': '4px solid', 'border-color': data.color, 'padding-left': '3px'}" style="margin:0px 0px 3px!important;font-weight: bold;padding-top: 2px;padding-bottom: 2px;"> {{data.key}}</p>
                                                                                        <div class="col col-lg-12 col-sm-12 col-md-12 col-xs-12 " style="padding: 0px 0px 0px 0px!important;">
                                                                                            <div class="col col-lg-5 col-sm-5 col-md-5 col-xs-5">
                                                                                                <p ng-show="data.showComparision==true" class="summaryFont" ng-class="{'summaryDoubleFont': aexppage.widgetData[widgetIndex].layoutOptionsX>1}"ng-style="{ 'font-family' : 'Montserrat-Black', 'text-transform':'uppercase', 'padding-left': '0px', 'border-right': '3px solid white','float':'left'}" style="margin:0px 0px 3px!important;font-weight: bold;"> {{data.summaryDisplay}}</p>
                                                                                            </div>
                                                                                            <div class="col col-lg-7 col-sm-7 col-md-7 col-xs-7" style="padding: 0px 0px 0px 0px!important;">
                                                                                                <!--<p ng-style="{'color': '#979797', 'font-family' : 'Montserrat-Regular','font-size':'12px', 'text-transform':'uppercase', 'background-color': 'whitesmoke'}" style="margin:0px 0px 3px!important;font-weight: bold;">{{data.key}} - <b ng-style="{'color':data.color, 'font-family' : 'Montserrat-Regular'}"> {{data.summaryDisplay}} </b></p>-->
                                                                                                <p ng-if="chart.options.chart.type != 'pieChart'" class="summaryKeyFont" ng-class="{'summaryKeyDoubleFont': aexppage.widgetData[widgetIndex].layoutOptionsX>1}" ng-show="data.showComparision==true" style="padding-top:3px;margin:1px 0px 1px!important;overflow: hidden;float: right;">{{data.period}}: <span ng-if="data.variance !=0">{{data.variance}}%  </span><i ng-if="data.arrow==='up' && data.variance !=0" style="font-size:0.8em;color:#2A8539" class="fa fa-arrow-up" aria-hidden="true"></i><i ng-if='data.arrow==="down" && data.variance !=0' style="font-size: 0.8em;color:#FF1B1B" class="fa fa-arrow-down" aria-hidden="true"></i></p>
                                                                                            </div>
                                                                                        </div>
                                                                                        <!--<p ng-if="chart.options.chart.type != 'pieChart'" style="font-size: 14px;font-family: Montserrat-Black;">{{data.period}}- <span ng-if="data.variance !=0">{{data.variance}}%</span><i ng-if="data.arrow==='up' && data.variance !=0" style="font-size: 0.8em;color:#2A8539" class="fa fa-arrow-up" aria-hidden="true"></i><i ng-if='data.arrow==="down" && data.variance !=0' style="font-size: 0.8em;color:#FF1B1B" class="fa fa-arrow-down" aria-hidden="true"></i></p>-->
                                                                                    </div>
                                                                                </li>
                                                                            </ul>
                                                                        </div>
