                                                                            <!--
                                                                                                                         ng-init="calculateChartHeight(widget.sizeY,chart.data.length,widgetIndex)">
                                                                            -->
                                                                            <!--<nvd3 options="chart.options" data="chart.data" api="chart.api" config="{refreshDataOnly:true, deepWatchDataDepth: 0}" events="events" ng-if="aexppage.widgetData[widgetIndex].visibility == true"></nvd3>-->
                                                                            <div id="{{'exportChartOptions-'+aexpageIndex+'-'+widgetIndex}}"   style="width:100%;height:100%;">
                                                                                <hc-chart options="chart.chartOptions"
                                                                                          id="{{'exportChartRepeat-'+aexpageIndex+'-'+widgetIndex+'-'+chartIndex}}"
                                                                                          ng-init="setChartSize(widgetIndex,chartIndex);"
                                                                                >Placeholder for generic chart
                                                                                </hc-chart>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div ng-repeat="(chartIndex,chart) in aexppage.widgetData[widgetIndex].chart" ng-if="chart.options.chart.type=== 'visitorAcquisitionEfficiency'" style="float: left;padding-left: 2px;padding-right: 2px;" ng-style="{'height': (100/aexppage.widgetData[widgetIndex].layoutOptionsY)+'%', 'width': (100/aexppage.widgetData[widgetIndex].layoutOptionsX)+'%'}">
                                                                        <div id="{{'exportUpdateHeight-'+aexpageIndex+'-'+widgetIndex}}" class="col col-lg-12 col-sm-12 col-md-12 col-xs-12" style="height:100%;padding-left:5px;padding-right:5px;">
                                                                            <div id="{{'exportChartOptions-'+aexpageIndex+'-'+widgetIndex}}"   style="width:100%;height:100%;">
                                                                                <hc-chart options="chart.chartOptions"
                                                                                          id="{{'exportChartRepeat-'+aexpageIndex+'-'+widgetIndex+'-'+chartIndex}}"
                                                                                          ng-init="setChartSize(widgetIndex,chartIndex);"
                                                                                >Placeholder for generic chart
                                                                                </hc-chart>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div ng-repeat="(chartIndex,chart) in aexppage.widgetData[widgetIndex].chart" ng-if="chart.options.chart.type=== 'mozoverview'" style="padding-left: 2px;padding-right: 2px;" ng-style="{'height': (100/aexppage.widgetData[widgetIndex].layoutOptionsY)+'%', 'width': (100/aexppage.widgetData[widgetIndex].layoutOptionsX)+'%'}">
                                                                        <div events="events" style="clear: both;height: 99%;" ng-if="aexppage.widgetData[widgetIndex].visibility == true" >
                                                                            <div class="scrollbar scrollInWidget" id="scroller">
