<div>
    <div gridster="gridsterOptions">
        <ul>
            <li gridster-item="widget" ng-repeat="widgets in dashboard.widgets">
                <div class="box" ng-controller="CustomWidgetController">
                    <div class="box-header">
                            <h3>{{widgets.name}}</h3>
                            <a title="settings" ng-click="openSettings(widgets)"><i class="fa fa-cog"></i></a>
                            <a title="Remove widget" ng-click="remove(widgets)"><i class="fa fa-minus-square"></i></a>
                    </div>
                    <div class="box-content"></div>
                </div>
            </li>
        </ul>
    </div>
    <div ui-view=""></div>
</div>





<div class="ibox-title">
    <h5>{{widgets.name}}</h5>
    <div class="ibox-tools">
        <a class="collapse-link">
            <i class="fa fa-chevron-up"></i>
        </a>
        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
            <i class="fa fa-wrench"></i>
        </a>
        <ul class="dropdown-menu dropdown-user">
            <li><a href="#">Config option 1</a>
            </li>
            <li><a href="#">Config option 2</a>
            </li>
        </ul>
        <a class="close-link">
            <i class="fa fa-times"></i>
        </a>
    </div>
</div>