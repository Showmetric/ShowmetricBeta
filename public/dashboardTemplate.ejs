<div ng-init="dashboardConfiguration()">
    <div id="dashboardLayout" class="dashboardLayout">
        <div class="border-bottom dashboardTitle white-bg">
            <div class="row titleMargin">
                <div class="col col-sm-12" id="CustomTemplate">
                    <div class="dashboardTemplate-items">
                        <div class="col-sm-3 dashboardName">
                            <a title="Click to rename this Dashboard" id='reset-btn' href="#" ng-model="dashboardName" editable-text="dashboard.dashboardName"   onaftersave="changeDashboardName()"  e-maxlength="35">
                                <pre ng-style="!dashboard.dashboardName  && {'color':'#999'}  || dashboard.dashboardName && {'color':'#000'}">{{dashboard.dashboardName || 'Name your dashboard'}}</pre>
                                <i class="fa fa-pencil hover-icon" aria-hidden="true" style="font-size: 1.5em;margin-top: 6px;color:#000;display:none"></i>
                            </a>
                        </div >
                        <div class="col-sm-4">
                            <div style="width:100%" class="daterange daterange--double three"></div>
                        </div>
                        <div class="col-sm-3 pull-right dashboardTitleIcons" id="dashboardTitleIcons" >
                            <form class="dashboardIcons autoarrange-export-delete">
                                <!--<button class="grey rightMargin auto-arrange"  ng-click="setAutoArrange()" ng-class="{'arrangeDisabled': loadedWidgetCount != dashboard.widgets.length}" ng-disabled="loadedWidgetCount != dashboard.widgets.length">
                                    Auto-arrange
                                </button>-->
                                <!--<button  class="grey rightMargin export"  ng-click="exportModal('app.reporting.dashboard.exportModal')" ng-class="{'exportDisabled': loadedWidgetCount != dashboard.widgets.length}" ng-disabled="loadedWidgetCount != dashboard.widgets.length" title="Export"></button>
                                <button  class="grey rightMargin delete"  ng-click="deleteDashboard()" title="Delete"></button>-->
                                <img src="https://cdn1.iconfinder.com/data/icons/networking-cool-vector-1/128/46-32.png"  ng-disabled="loadedWidgetCount != dashboard.widgets.length" ng-click="(loadedWidgetCount != dashboard.widgets.length) || setAutoArrange()" title="Auto-arrange">
                                <img src=" https://cdn0.iconfinder.com/data/icons/design-and-development-cool-vector-2/128/75-32.png"  ng-disabled="loadedWidgetCount != dashboard.widgets.length" ng-click="(loadedWidgetCount != dashboard.widgets.length) || exportModal('app.reporting.dashboard.exportModal')" title="Export">
                                <img src=" https://cdn0.iconfinder.com/data/icons/design-and-development-cool-vector-3/128/102-32.png" ng-click="deleteDashboard()" title="Delete">
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="parallel-dashboard greyish-bg" ng-style="{'height': docHeight+'px'}" ng-if="widgetsPresent == false">
            <a ui-sref="app.reporting.dashboard.basicWidget({widgetType:'basic'})"><div class="white-bg empty-dashboard"><span>Add your first widget</span></div></a>
        </div>
        <div id="dashboardContent"  ng-style="{'height': docHeight+'px'}" class="greyish-bg dashboardContent" ng-if="widgetsPresent == true" >
            <div class="spiner-mobile-example" ng-if="loadedWidgetCount != dashboard.widgets.length">
                <div class="sk-spinner sk-spinner-double-bounce">
                    <div class="sk-double-bounce1"></div>
                    <div class="sk-double-bounce2"></div>
                </div>
            </div>
            <div class="spiner-example" ng-if="spinerEnable==true">
                <div class="sk-spinner sk-spinner-double-bounce">
                    <div class="sk-double-bounce1"></div>
                    <div class="sk-double-bounce2"></div>
                </div>
            </div>
            <div gridster="gridsterOptions" class="row gridsterStlyeOptions" id="gridsterItems" ng-hide="(isMobile)&&(loadedWidgetCount != dashboard.widgets.length)"><!--ng-switch-when="true"-->
                <ul>
                    <li gridster-item="widget" ng-repeat="(widgetIndex,widget) in dashboard.widgets">
                        <div class="box getBox-{{widget.id}}" style=" border: 0px !important;" ng-controller="CustomWidgetController" id="{{widget.id}}">
                            <div class="box-header" id="getWidgetColor-{{widget.id}}" ref="{{dashboard.widgetData[widgetIndex].color}}" style="background-color: #ffffff;"> <!--ng-style="{'background-color': dashboard.widgetData[widgetIndex].color}"-->
                                <div style="float: left;width: 93%;">
                                    <img src="image/thumbnail/{{widget.channelName}}.png" style="float:left;vertical-align: super;max-height: 31px; max-width: 50px; padding:5px"/>
                                    <h3 ng-style="widget.sizeX!=1  && {'width':'80%'}  || widget.sizeX==1 && {'width':'50%'} "><a title="Click to Rename this widget" style="float:left;border-bottom:0px!important;padding:7px!important;color:#000!important" href="#" editable-text="dashboard.widgetData[widgetIndex].name" buttons="no" onaftersave="changeWidgetName(widget,dashboard.widgetData[widgetIndex].name)"  e-maxlength="35">
                                            <span>{{dashboard.widgetData[widgetIndex].name || 'Name your widget'}}</span>
                                            <!--
                                              <i class="fa fa-pencil hover-icon" aria-hidden="true" ng-style="widget.sizeX>2  &&{'margin-top': '-15px','float':'right','margin-left': '100%'} || widget.sizeX==1  &&{'margin-top': '-16px','float':'none','position': 'relative','margin-left': '5%'} || widget.sizeX==2  &&{'margin-top': '-15px','float':'none','margin-left': '125px'}" style="font-size: 1em;color:#000;display:none"></i>
                                            -->
                                        </a>
                                    </h3>
                                </div>
                                <div ng-hide="widget.sizeX === 1" class="dropdown"  id="widget-dropdown-{{widget.id}}" style="float: right; margin-top: 3px;margin-right: -25px;">
                                    <a title="settings" ng-click="showSettings(widget.id);"><i class="fa fa-cog"></i></a>
                                    <div id="settingsDropdown-{{widget.id}}" class="dropdown-content">
                                        <a title="Alerts" ng-click="alertModal('app.reporting.dashboard.alertModal',widget)" ng-hide="widget.widgetType == 'customFusion' || widget.isAlert == false"><i class="fa fa-bell-o"></i> Alerts</a>
                                        <a title="Toggle Legends" ng-click="toggleLegends(widget.id)"><i class="fa fa-filter"></i> Filter</a>
                                        <a title="Delete Widget" ng-click="removeWid(widget);deleteWidget(widget)"><i class="glyphicon glyphicon-trash favigon-img"></i>Delete</a>
                                        <a title="Click to fuse this widget with other widgets" ng-show="widget.widgetType != 'customFusion'" ng-click="openSettings(widget)"><i class="fa fa-random"></i> Fusion</a>
                                        <a title="Click to export this widget in PNG format"  ng-click="exportWidgetInPng(widget.id,dashboard.widgetData[widgetIndex].name)"><i class="fa fa-download"></i> &nbsp;PNG &nbsp;  </a>
                                        <a title="Find out how to send data for custom widgets - Documentation" ng-if="widget.widgetType==='custom'" href="/customDataDocumentation" target="_blank" class="findOutHow">Custom Documentation</a>
                                    </div>
                                </div>
                            </div>

                            <div class="box-content" style="vertical-align: middle" ng-style="{position:(isMobile?'static':'absolute')}">
                                <div ng-if="dashboard.widgetData[widgetIndex].visibility == false" style="height:100%;">
                                    <div class="spiner-example" id="widgetData-{{widget.id}}">
                                        <div class="sk-spinner sk-spinner-double-bounce">
                                            <div class="sk-double-bounce1"></div>
                                            <div class="sk-double-bounce2"></div>
                                        </div>
                                    </div>
                                    <div class="errorWidgetData" id="errorWidgetData-{{widget.id}}">
                                        <div class="row" style="margin: 0px;height: 100%">
                                            <div style="height:99%;">
                                                <div style="position:relative; top:36%; text-align:center; padding-right:20px">
                                                    <p style="word-wrap:break-word;font-size: 10px;">Your credential is insufficient to fetch data for this metric</p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class=" errorWidgetData  background-color" id="errorWidgetTokenexpire-{{widget.id}}" style="margin-top: 0%;">
                                        <div class="row" style="margin: 0px;height: 100%">
                                            <div style="height:99%;">
                                                <div style="position:relative; top:25%; text-align:center; padding-right:20px">
                                                    <p style="word-wrap:break-word;font-size: 10px;">Access to one or more profiles has expired</p>
                                                    <a ui-sref="app.reporting.listProfile"><i style="color: #23527c;" class="fa fa-refresh" aria-hidden="true"> </i><p style="color:#000;font-size: 10px">  Refresh</p></a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row getWidgetGraph" style="height: 100%;margin:0px;" ng-if="dashboard.widgetData[widgetIndex].visibility == true">
                                    <div ng-repeat="(chartIndex,chart) in dashboard.widgetData[widgetIndex].chart" ng-if="chart.options.chart.type=== 'lineChart' || chart.options.chart.type=== 'engagedUsersReach' || chart.options.chart.type=== 'pieChart' || chart.options.chart.type=== 'reachVsImpressions' || chart.options.chart.type=== 'multiBarChart' || chart.options.chart.type=== 'multiChart'" style="float: left;padding-left: 2px;padding-right: 2px;" ng-style="{'height': (100/dashboard.widgetData[widgetIndex].layoutOptionsY)+'%', 'width': (100/dashboard.widgetData[widgetIndex].layoutOptionsX)+'%'}">
                                        <div class="col col-lg-4 col-sm-4 col-md-4 scrollbar scroller" ng-if="dashboard.widgetData[widgetIndex].showSummary !=false" style="margin:0px;height:100%;padding-left:5px;padding-right:5px;"><!--  ng-style="calculateSummaryHeight(widget.sizeY,chart.data.length)">-->
                                            <ul style="height:100%;font-size:12px;padding-left: 5px;">
                                                <li  ng-repeat="data in chart.data" ng-class="{'summaryAlign': chart.data.length<=3}" style="text-align: center;padding-top: 1px;padding-left:0px;padding-right: 2px;font-size: 1.0em!important;" id="li-{{widget.id}}-{{$index}}" class="col col-lg-12" ng-style="calculateRowHeight(data,chart.data.length,widget.sizeX,widget.sizeY,dashboard.widgetData[widgetIndex].layoutOptionsX)"><!--size-watcher-width="data.mywidth"--><!--ng-class="calculateColumnWidth(chart.data.length,widget.sizeX,dashboard.widgetData[widgetIndex].layoutOptionsX)" ng-style="calculateRowHeight(data,chart.data.length,widget.sizeX,widget.sizeY,dashboard.widgetData[widgetIndex].layoutOptionsX)"-->
                                                    <div style="margin: 0px;height: 100%;overflow: hidden;white-space: nowrap;color: #000000;text-align: center;" >
                                                        <p ng-show="data.showComparision==false" ng-style="{ 'font-family' : 'Heebo-Medium','font-size':'10px', 'text-transform':'uppercase',  'border-bottom': '4px solid', 'border-color': data.color, 'padding-left': '3px'}" style="margin:0px 0px 3px!important;font-weight: bold;padding-top: 2px;padding-bottom: 2px;"> {{data.key}} - {{data.summaryDisplay}}</p>
                                                        <p ng-show="data.showComparision==true" ng-style="{ 'font-family' : 'Heebo-Medium','font-size':'14px', 'text-transform':'uppercase',  'border-bottom': '4px solid', 'border-color': data.color, 'padding-left': '3px'}" style="margin:0px 0px 3px!important;font-weight: bold;padding-top: 2px;padding-bottom: 2px;"> {{data.key}}</p>
                                                        <div class="col col-lg-12 col-sm-12 col-md-12 col-xs-12 " style="padding: 0px 0px 0px 0px!important;">
                                                            <div class="col col-lg-5 col-sm-5 col-md-5 col-xs-5">
                                                                 <p ng-show="data.showComparision==true" class="summaryFont" ng-class="{'summaryDoubleFont': dashboard.widgetData[widgetIndex].layoutOptionsX>1}"ng-style="{ 'font-family' : 'Heebo-Medium', 'text-transform':'uppercase', 'padding-left': '0px', 'border-right': '3px solid white','float':'left'}" style="margin:0px 0px 3px!important;font-weight: bold;"> {{data.summaryDisplay}}</p>
                                                            </div>
                                                            <div class="col col-lg-7 col-sm-7 col-md-7 col-xs-7" style="padding: 0px 0px 0px 0px!important;">
                                                                <!--<p ng-style="{'color': '#979797', 'font-family' : 'Heebo-Medium','font-size':'12px', 'text-transform':'uppercase', 'background-color': 'whitesmoke'}" style="margin:0px 0px 3px!important;font-weight: bold;">{{data.key}} - <b ng-style="{'color':data.color, 'font-family' : 'Heebo-Medium'}"> {{data.summaryDisplay}} </b></p>-->
                                                                <p ng-if="chart.options.chart.type != 'pieChart'" class="summaryKeyFont" ng-class="{'summaryKeyDoubleFont': dashboard.widgetData[widgetIndex].layoutOptionsX>1}" ng-show="data.showComparision==true" style="padding-top:3px;margin:1px 0px 1px!important;overflow: hidden;float: right;">{{data.period}}: <span ng-if="data.variance !=0">{{data.variance}}%  </span><i ng-if="data.arrow==='up' && data.variance !=0" style="font-size:0.8em;color:#2A8539" class="fa fa-arrow-up" aria-hidden="true"></i><i ng-if='data.arrow==="down" && data.variance !=0' style="font-size: 0.8em;color:#FF1B1B" class="fa fa-arrow-down" aria-hidden="true"></i></p>
                                                            </div>
                                                        </div>
                                                           <!--<p ng-if="chart.options.chart.type != 'pieChart'" style="font-size: 14px;font-family: Heebo-Light;">{{data.period}}- <span ng-if="data.variance !=0">{{data.variance}}%</span><i ng-if="data.arrow==='up' && data.variance !=0" style="font-size: 0.8em;color:#2A8539" class="fa fa-arrow-up" aria-hidden="true"></i><i ng-if='data.arrow==="down" && data.variance !=0' style="font-size: 0.8em;color:#FF1B1B" class="fa fa-arrow-down" aria-hidden="true"></i></p>-->
                                                    </div>
                                                </li>
                                            </ul>
                                        </div>
                                        <div id="{{'updateHeight'+widgetIndex}}" ng-class="{'col col-lg-8 col-sm-8 col-md-8':dashboard.widgetData[widgetIndex].showSummary !=false}" ng-class="{'col col-lg-12 col-sm-12 col-md-12':dashboard.widgetData[widgetIndex].showSummary ===false}" style="height:100%;padding-left:5px;padding-right:5px;">
<!--
                                             ng-init="calculateChartHeight(widget.sizeY,chart.data.length,widgetIndex)">
-->
                                            <!--<nvd3 options="chart.options" data="chart.data" api="chart.api" config="{refreshDataOnly:true, deepWatchDataDepth: 0}" events="events" ng-if="dashboard.widgetData[widgetIndex].visibility == true"></nvd3>-->
                                            <div id="{{'chartOptions'+widgetIndex}}"   style="width:100%;height:100%;">
                                                <hc-chart options="chart.chartOptions"
                                                          id="{{'chartRepeat'+widgetIndex+'-'+chartIndex}}"
                                                          ng-init="setChartSize(widgetIndex,chartIndex);"
                                                >Placeholder for generic chart
                                                </hc-chart>
                                            </div>
                                        </div>
                                    </div>
                                    <div ng-repeat="(chartIndex,chart) in dashboard.widgetData[widgetIndex].chart" ng-if="chart.options.chart.type=== 'visitorAcquisitionEfficiency'" style="float: left;padding-left: 2px;padding-right: 2px;" ng-style="{'height': (100/dashboard.widgetData[widgetIndex].layoutOptionsY)+'%', 'width': (100/dashboard.widgetData[widgetIndex].layoutOptionsX)+'%'}">
                                        <div class="col col-lg-4 col-sm-4 col-md-4 scrollbar scroller" style="margin:0px;height:100%;padding-left:5px;padding-right:5px;"><!--  ng-style="calculateSummaryHeight(widget.sizeY,chart.data.length)">-->
                                            <ul style="padding-top: 25%;height:100%;font-size:12px;padding-left: 5px;">
                                                <li  ng-repeat="data in chart.data[0].summary" style="text-align: center;padding-top: 1px;padding-left:0px;padding-right: 2px;font-size: 1.0em!important;" id="li-{{widget.id}}-{{$index}}" class="col col-lg-12" >
                                                    <p ng-style="{ 'font-family' : 'Heebo-Medium','font-size':'14px', 'text-transform':'uppercase',  'border-bottom': '4px solid', 'border-color': chart.data[0].color[$index], 'padding-left': '3px'}" style="margin:0px 0px 3px!important;font-weight: bold;padding-top: 2px;padding-bottom: 2px;"> {{data.key}}</p>
                                                    <p class="summaryFont" ng-class="{'summaryDoubleFont': dashboard.widgetData[widgetIndex].layoutOptionsX>1}"ng-style="{ 'font-family' : 'Heebo-Medium', 'text-transform':'uppercase', 'padding-left': '0px', 'border-right': '3px solid white'}" style="margin:0px 0px 3px!important;font-weight: bold;"> {{data.value}}</p>
                                                </li>
                                            </ul>
                                        </div>
                                        <div id="{{'updateHeight'+widgetIndex}}" class="col col-lg-8 col-sm-8 col-md-8" style="height:100%;padding-left:5px;padding-right:5px;">
                                            <div id="{{'chartOptions'+widgetIndex}}"   style="width:100%;height:100%;">
                                                <hc-chart options="chart.chartOptions"
                                                          id="{{'chartRepeat'+widgetIndex+'-'+chartIndex}}"
                                                          ng-init="setChartSize(widgetIndex,chartIndex);"
                                                >Placeholder for generic chart
                                                </hc-chart>
                                            </div>
                                        </div>
                                    </div>
                                    <div ng-repeat="(chartIndex,chart) in dashboard.widgetData[widgetIndex].chart" ng-if="chart.options.chart.type=== 'mozoverview'" style="padding-left: 2px;padding-right: 2px;" ng-style="{'height': (100/dashboard.widgetData[widgetIndex].layoutOptionsY)+'%', 'width': (100/dashboard.widgetData[widgetIndex].layoutOptionsX)+'%'}">
                                        <div events="events" style="clear: both;height: 99%;" ng-if="dashboard.widgetData[widgetIndex].visibility == true" >
                                            <div class="scrollbar scrollInWidget" id="scroller">
                                                <table class="table table-striped">
                                                    <tr style="padding:2px 5px;width:100%">
                                                        <th style="padding:5px 5px;width:55%;">Moz Rank</th>
                                                        <th style="padding:5px 5px;width:15%;text-align:right;">{{chart.displayData.mozRankURL}}</th>
                                                    </tr>
                                                    <tr style="padding:2px 5px;width:100%">
                                                        <th style="padding:5px 5px;width:55%;">External Links</th>
                                                        <th style="padding:5px 5px;width:15%;text-align:right;">{{chart.displayData.externalEquityLinks}}</th>
                                                    </tr>
                                                    <tr style="padding:2px 5px;width:100%">
                                                        <th style="padding:5px 5px;width:55%;">Back Links</th>
                                                        <th style="padding:5px 5px;width:15%;text-align:right;">{{chart.displayData.links}}</th>
                                                    </tr>
                                                    <tr style="padding:2px 5px;width:100%">
                                                        <th style="padding:5px 5px;width:55%;">domain Authority</th>
                                                        <th style="padding:5px 5px;width:15%;text-align:right;">{{chart.displayData.domainageAuthority}}</th>
                                                    </tr>
                                                    <tr style="padding:2px 5px;width:100%">
                                                        <th style="padding:5px 5px;width:55%;">Page Authority</th>
                                                        <th style="padding:5px 5px;width:15%;text-align:right;">{{chart.displayData.pageAuthority}}</th>
                                                    </tr>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                    <div ng-repeat="chart in dashboard.widgetData[widgetIndex].chart" ng-if="chart.options.chart.type=== 'emptyCharts'" style="float: left;padding-left: 2px;padding-right: 2px;" ng-style="{'height': (100/dashboard.widgetData[widgetIndex].layoutOptionsY)+'%', 'width': (100/dashboard.widgetData[widgetIndex].layoutOptionsX)+'%'}">
                                        <div class="row" style="margin: 0px;height: 100%">
                                            <div events="events" style="height: 99%;" ng-if="dashboard.widgetData[widgetIndex].visibility == true">
                                                <div style="position:relative; top: 36%; text-align: center" ng-repeat="data in chart.data">
                                                    <p style="word-wrap:break-word;">{{data.message}}</p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div ng-repeat="chart in dashboard.widgetData[widgetIndex].chart" ng-if="chart.options.chart.type=== 'instagramPosts'" style="float: left;padding-left: 2px;padding-right: 2px;" ng-style="{'height': (100/dashboard.widgetData[widgetIndex].layoutOptionsY)+'%', 'width': (100/dashboard.widgetData[widgetIndex].layoutOptionsX)+'%'}">
                                        <div class="row" style="margin: 0px;height: 100%" id='chartTable-{{widget.id}}'>
                                            <div events="events" style="height: 99%" ng-if="dashboard.widgetData[widgetIndex].visibility == true">
                                                <div class="scrollbar scrollInWidget" id="scroller">
                                                    <div class="col-lg-12 listed"  ng-class-odd="'odd'" ng-class-even="'even'" ng-class="{responsiveInstagramListed:widget.sizeX<2}" ng-repeat="data in chart.data">
                                                        <div class="col-lg-1 listed-image" >
                                                            <a  href="{{data.links}}" target="_blank"><img ng-src="{{data.image}}" height="50px" widget="50px"/></a>
                                                        </div>
                                                        <div class="col-lg-11 aside">
                                                            <div class="postComment"><a class="apiLink" href="{{data.links}} " target="_blank">{{data.postComment}}</a></div>
                                                            <div class="impression">
                                                                <div class="likes"><span>likes:{{data.likes}}</span></div>
                                                                <div class="comment"><span>Comments:{{data.comments}}</span></div>
                                                                <div class="date"><span class="date">{{data.date}}</span></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div ng-repeat="chart in dashboard.widgetData[widgetIndex].chart" ng-if="chart.options.chart.type=== 'highEngagementTweets'" style="float: left;padding-left: 2px;padding-right: 2px;" ng-style="{'height': (100/dashboard.widgetData[widgetIndex].layoutOptionsY)+'%', 'width': (100/dashboard.widgetData[widgetIndex].layoutOptionsX)+'%'}">
                                        <div class="row" style="margin: 0px;height: 100%" id='chartTable-{{widget.id}}'>
                                            <div events="events" style="height: 99%" ng-if="dashboard.widgetData[widgetIndex].visibility == true">
                                                <div class="scrollbar scrollInWidget" id="scroller">
                                                    <div class="col-lg-12 listed"  ng-class-odd="'odd'" ng-class-even="'even'" ng-repeat="data in chart.data" ng-if="data.postComment">
                                                        <div class="col-lg-12">
                                                            <div class="postComment"><a ng-if="data.links.media" class="apiLink" href="{{data.links.media[0].expanded_url}} " target="_blank">{{data.postComment}}</a><a ng-if="!data.links.media[0]" class="apiLink" href="https://twitter.com" target="_blank">{{data.postComment}}</a></div>
                                                            <div class="impression">
                                                                <div class="likes"><span>Likes: {{data.likes}}</span></div>
                                                                <div class="comment"><span>Retweets: {{data.reTweet}}</span></div>
                                                                <div class="date"><span>{{data.date}}</span></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div ng-repeat="chart in dashboard.widgetData[widgetIndex].chart" ng-if="chart.options.chart.type=== 'highestEngagementLinkedIn'" style="float: left;padding-left: 2px;padding-right: 2px;" ng-style="{'height': (100/dashboard.widgetData[widgetIndex].layoutOptionsY)+'%', 'width': (100/dashboard.widgetData[widgetIndex].layoutOptionsX)+'%'}">
                                        <div class="row" style="margin: 0px;height: 100%" id='chartTable-{{widget.id}}'>
                                            <div events="events" style="height: 99%" ng-if="dashboard.widgetData[widgetIndex].visibility == true">
                                                <div class="scrollbar scrollInWidget" id="scroller">
                                                    <div class="col-lg-12 listed"  ng-class-odd="'odd'" ng-class-even="'even'" ng-class="{responsiveListed:widget.sizeX<2}" ng-repeat="data in chart.data" ng-if="data.postComment">
                                                        <div class="col-lg-12">
                                                            <div class="postComment"><a class="apiLink" href="{{data.link}} " target="_blank">{{data.postComment}}</a></div>
                                                            <div class="impression">
                                                                <div class="likes"><span>Likes: {{data.likes}}</span></div>
                                                                <div class="comment"><span>comments: {{data.comments}}</span></div>
                                                                <div class="comment"><span>shares: {{data.shares}}</span></div>
                                                                <div class="comment"><span>clicks: {{data.clicks}}</span></div>
                                                                <div class="comment"><span>impressions: {{data.impressions}}</span></div>
                                                                <div ng-style="widget.sizeX<=2  && {'float':'left'}  || widget.sizeX>2 && {'float':'right'}"><span>{{data.date}}</span></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!--<div class="col-lg-12" style="position: relative;top:40%;text-align: center;" ng-if="!chart.data.postComment">-->
                                                    <!--<span >No Updates available</span>-->
                                                    <!--</div>-->
                                                </div>
                                            </div>
