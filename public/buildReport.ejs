                <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2" style='color:#666;text-style:underline' ng-if="report.isDraft == true || disableFilter!=false" ng-hide="report.isDraft != true || disableFilter==false" ><span style="background-color: #ddd"> Draft </span></div>
                <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2" style='color:red;text-style:underline' ng-if="report.isDraft != true || disableFilter==false" ng-show="report.isDraft != true || disableFilter==false" ></div>
                <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2"><a title="Edit this Report" ng-click="editReport(report)" ng-hide="report.dashboardDeleted == true"><i class="fa fa-pencil-square-o" aria-hidden="true" style="color: #ff6c3a"></i> Edit &nbsp; </a></div>
                <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2"><a title="Delete Widget" ng-click="deleteReport(report)"><i class="glyphicon glyphicon-trash favigon-img" style="color: #ff6c3a"></i>Delete</a></div>
                <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2"><a title="Click to open PDF of this report" ng-show="report.pdfLink != null" ng-click="openReportPDF(report)"><i class="fa fa-print" aria-hidden="true" style="color: #ff6c3a"></i> &nbsp;PDF &nbsp;  </a></div>
            </div>
        </div>
    </div>
