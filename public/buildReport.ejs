<div ng-if='newReportActive==true' ng-init="fetchAllDashboards()" class="wrapper wrapper-content postitionRelative greyish-bg">
    <div ng-if="gridloading === true"  class="spiner-example">
        <div class="sk-spinner sk-spinner-double-bounce">
            <div class="sk-double-bounce1"></div>
            <div class="sk-double-bounce2"></div>
        </div>
    </div>

    <div class="row" ng-show="dashboardList != null && gridloading === false">
        <div class="col col-sm-12 col-md-12 col-lg-12 center dashboardTemplate-items">
            <p style="color:#000;font-size:18px;font-family:Montserrat-Medium;padding:5px">Select Dashboards:</p>
        </div>
    </div>
    <div class="gridAlign scroller"  ng-show="dashboardList != null && gridloading === false " ng-style="{'height': dashboardHeight+'px'}">
        <div class="col col-lg-2 grid-font grid-text"  ng-repeat="dashboard in dashboardList" ng-click="selectDashboard(dashboard)" ng-style="dashboard.isSelected==true  && {'border': '2px solid #04509B'}  || dashboard.isSelected==true && {'border': '0px solid #eaeff3'} " >
            <div style="height: 10px; cursor: pointer;" ng-if="dashboard.isSelected==true" class="col-lg-1 pull-right"><i class="fa fa-check" style="color: green" aria-hidden="true"></i></div>
            <div style="height: 40px" class="contentName">
                <div class="dashboards col-lg-2"  ng-if="(dashboard.name == null || dashboard.name==' ')" >
                    <a style="color:#9A9A9A">Unnamed Dashboard</a>
                </div>

                <div class="dashboards col-lg-2" ng-if="dashboard.name != null">
                    <a style="color:#000">{{dashboard.name}}</a>
                </div>
            </div>
        </div>
        <div class="center grid-font" ng-if="dashboardList.length===0 || dashboardList==null "><p>You don't have access to any dashboards yet</p></div>
    </div>
