            </div>
        </div>
        <div class="row reportListingGrid"  ng-class-odd="'odd'" ng-class-even="'even'" ng-repeat="report in reportList| filter: (!disableFilter || '') &&  myFilter">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div ng-if="(report.name == null || report.name=='')" ng-show="(report.name == null || report.name=='')" class="col-xs-4col-sm-4 col-md-4 col-lg-4">Untitled Report</div>
