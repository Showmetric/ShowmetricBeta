<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10" style="float:right">
        <div class="btn btn-primary" style="margin-top:13px;float:right;">
            <i class="fa fa-plus"></i>
            <span> Add a Recommended Dashboard </span>
        </div>
        <div class="btn btn-primary" style="margin-top:13px;float:right;margin-right:5px;">
            <i class="fa fa-plus"></i>
            <span ui-sref-active="active"><a ui-sref="dashboards.dashboard_3" style="color:white;">Add an Empty Dashboard</a> </span>
        </div>

    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight" ng-controller="flotChartCtrl as charts">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <div class="col-md-3">
                        <div class="input-group" data-autoclose="true">
                            <input style="border-right:none;" class="form-control remove-border" type="text" value="" name="account" placeholder="Account 1">
                  <span class="input-group-addon remove-border">
				  <i class="fa fa-edit"></i>

                  </span>

                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <label for="daterange3" class="control-label">Picker with custom locale</label>
                            <input date-range-picker id="daterange3" name="daterange3" class="form-control date-picker" type="text"
                                   ng-model="date" options="opts" required/>
                        </div>
                    </div>
                    <div ibox-tools></div>
                </div>
                <div class="ibox-content">
                    <div class="flot-chart">
                        <div flot class="flot-chart-content" dataset="charts.flotMultiData" options="charts.flotMultiOptions"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


