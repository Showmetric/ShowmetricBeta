<div ng-init="customReportConfiguration()"  id="reportLayout" class="dashboardLayout">
    <div ng-if="stepActive=='step1'&& !reportNotSupported" ng-show="stepActive=='step1'&& !reportNotSupported" class="border-bottom dashboardTitle darkblue-bg">
        <div class="row" style="height:100%">
            <div class="col col-xs-24 col-sm-12 col-md-12 col-lg-12" style="background: #232c3b">
                <div class="dashboardTemplate-items">
                    <div class="col-sm-2 col-md-2 col-lg-2 reportText-header">
                        <p>Report Builder</p>
                    </div >
                    <div class="col-xs-6 col-sm-3 col-md-3 col-lg-3 reportName" style='color:#000'>
                        <a title="Click to rename this Report" id='reset-btn' href="#" ng-model="reportName" editable-text="customReport.reportName"   onaftersave="changeReportName()"  e-maxlength="25">
                            <pre ng-style="!customReport.reportName  && {'color':'#fff'}  || customReport.reportName && {'color':'#fff'}">{{customReport.reportName || 'Name your Report'}}</pre>
                            <i class="fa fa-pencil hover-icon" aria-hidden="true" style="font-size: 1.5em;margin-top: 6px;color:#ff6c3a;display:none"></i>
                        </a>
                    </div >
                    <div class="col-xs-6 col-sm-3 col-md-3 col-lg-3 ">
                        <div style="width:100%" class="daterange daterange--double three"></div>
                    </div>
                    <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4 text-right">
                        <button class="orange rightMargin add-text"  ng-click="updateWidgetsInDb()">
                            <i class="fa fa-floppy-o " aria-hidden="true"></i> &nbsp;Save
                        </button>
                        <button class="orange rightMargin add-text" ng-class="{'exportDisabled': loadedWidgetCount != widgetDataLength}" ng-disabled="loadedWidgetCount != widgetDataLength"  ng-click="reportPdfPreview('step2')"><!-- ng-class="{'exportDisabled': loadedWidgetCount != customReport.widgets.length}" ng-disabled="loadedWidgetCount != customReport.widgets.length" -->
                            <i class="fa fa-arrow-right" aria-hidden="true"></i> Next
                        </button>
                        <!--<div style='width:100%' class="daterange daterange&#45;&#45;double three"></div>-->
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div ng-if="stepActive=='step2'&& !reportNotSupported" ng-show="stepActive=='step2'&& !reportNotSupported" class="border-bottom dashboardTitle darkblue-bg">
        <div class="row" style="height:100%">
            <div class="col col-xs-24 col-sm-12 col-md-12 col-lg-12">
                <div class="dashboardTemplate-items">
                    <div class="col-xs-4 col-sm-2 col-md-2 col-lg-2 reportText-header">
                        <p>Report Builder</p>
                    </div >
                    <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 reportName" style='color:#000'>
                        <a title="Click to rename this Report" id='reset-btn' href="#" ng-model="reportName" editable-text="customReport.reportName"   onaftersave="changeReportName()"  e-maxlength="25">
                            <pre ng-style="!customReport.reportName  && {'color':'#fff'}  || customReport.reportName && {'color':'#fff'}">{{customReport.reportName || 'Name your Report'}}</pre>
                            <i class="fa fa-pencil hover-icon" aria-hidden="true" style="font-size: 1.5em;margin-top: 6px;color:#ff6c3a;display:none"></i>
                        </a>
                    </div >
                    <div class="col-xs-8 col-sm-4 col-md-4 col-lg-4 text-right">
                        <button class="orange rightMargin add-text"  ng-click="reportPdfPreview('step1')">
                            <i class="fa fa-arrow-left" aria-hidden="true"></i>&nbsp;Back
                        </button>
                        <button class="orange rightMargin add-text"  ng-click="reportPdfPreview('step3')"><!-- ng-class="{'exportDisabled': loadedWidgetCount != customReport.widgets.length}" ng-disabled="loadedWidgetCount != customReport.widgets.length" -->
                            <i class="fa fa-arrow-right" aria-hidden="true"></i>&nbsp;Next
                        </button>
                        <!--<div style='width:100%' class="daterange daterange&#45;&#45;double three"></div>-->
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div ng-if="stepActive=='step3'&& !reportNotSupported" ng-show="stepActive=='step3'&& !reportNotSupported" class="border-bottom dashboardTitle darkblue-bg">
        <div class="row" style="height:100%">
            <div class="col col-xs-24 col-sm-12 col-md-12 col-lg-12">
                <div class="dashboardTemplate-items">
                    <div class="col-sm-4 col-md-4 col-lg-4 reportText-header">
                        <p >Print Preview</p>
                    </div >
                    <div class="col-xs-8 col-sm-4 col-md-4 col-lg-4 reportName" >
                        <p  ng-style="!customReport.reportName  && {'color':'#999'}  || customReport.reportName && {'color':'#fff'}">{{customReport.reportName || 'Name your Report'}}</p>
                    </div >
                    <div class="col-xs-8 col-sm-4 col-md-4 col-lg-4 text-right">
                        <button class="orange rightMargin add-text"  ng-click="reportPdfPreview('step2')" id="reportEditButton">
                            <i class="fa fa-pencil-square-o" aria-hidden="true"></i>&nbsp;Edit
                        </button>
                        <button class="orange rightMargin add-text"  id="reportDownloadButton" ng-click="exportReportinPDF()"><!-- ng-class="{'exportDisabled': loadedWidgetCount != customReport.widgets.length}" ng-disabled="loadedWidgetCount != customReport.widgets.length" -->
                            <i class="fa fa-download" aria-hidden="true"></i>&nbsp;Download
                        </button>
                        <!--<div style='width:100%' class="daterange daterange&#45;&#45;double three"></div>-->
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div ng-if="stepActive=='error'|| reportNotSupported" ng-show="stepActive=='error'|| reportNotSupported" class="border-bottom dashboardTitle darkblue-bg">
        <div class="row" style="height:100%">
            <div class="col col-xs-24 col-sm-12 col-md-12 col-lg-12 ">
                <p style="color:#fff;margin-top:5px;font-size:18px;font-family:Montserrat-Bold;padding:5px">Report Builder</p>
                <!--<div style='width:100%' class="daterange daterange&#45;&#45;double three"></div>-->
            </div>
        </div>
    </div>
    <div  class="greyish-bg reportLayoutContent" ng-style="{'height': docHeight+'px'}">
        <div ng-show="reportLoading === true"  class="spiner-example">
            <div class="sk-spinner sk-spinner-double-bounce">
                <div class="sk-double-bounce1"></div>
                <div class="sk-double-bounce2"></div>
            </div>
        </div>
        <div class="reportDashboardContent" ng-if="reportLoading==false" ng-show="reportLoading === false "  ng-style="{'left': sideSpace+'px','right': sideSpace+'px'}">
            <div ng-if="stepActive=='step1' && !reportNotSupported" ng-show="stepActive=='step1'&& !reportNotSupported" gridster="gridsterOptions" class="row center white-bg gridsterStlyeOptions"  id="gridsterItemsInReport"><!--ng-repeat="pages in totalPagesinReport" id="gridsterItems-{{pages}}"ng-switch-when="true"-->
                <ul>
                    <li gridster-item="empWidget" ><!--ng-if="widget.pageNumber==pages"-->
                        <div class="box darkblue-bg" style=" border: 0px !important;">
                            <div class='pagebreak-header'> <!--ng-style="{'background-color': customReport.widgetData[widget.id].color}"-->
                                <div style="text-align:center;padding:5px 0px">
                                    <h3>Available Options</h3>
                                </div>
                            </div>
                            <div class="box-content" style="top:30px !important;padding: 10px !important;vertical-align: middle" ng-style="{position:(isMobile?'static':'absolute')}">
                                <div class="row " style="height: 100%;margin:0px;">
                                    <div class="col col-sm-12 col-md-12 col-lg-12" style="margin-top:2px">
                                        <div class="col-sm-12 col-md-12 col-lg-12 text-center">
                                            <button class="orange rightMargin add-text"  ng-click="addTextWidget('reportHeadingWidget',0,0,1)" ><!-- ng-class="{'exportDisabled': loadedWidgetCount != customReport.widgets.length}" ng-disabled="loadedWidgetCount != customReport.widgets.length" -->
                                                <i class="fa fa-header" aria-hidden="true"></i>&nbsp;Add Title
                                            </button>
                                            <button class="orange rightMargin add-text"  ng-click="addTextWidget('reportParaWidget',0,0,1)"><!-- ng-class="{'exportDisabled': loadedWidgetCount != customReport.widgets.length}" ng-disabled="loadedWidgetCount != customReport.widgets.length" -->
                                                <i class="fa fa-paragraph" aria-hidden="true"></i>&nbsp;Add Text
                                            </button>
                                        </div >
                                        <!--<div class="col-sm-6 col-md-6 col-lg-6 text-center">-->
                                        <!---->
                                        <!--&lt;!&ndash;<div style='width:100%' class="daterange daterange&#45;&#45;double three"></div>&ndash;&gt;-->
                                        <!--</div>-->

                                    </div>
                                </div>
                            </div>
                        </div>
                    </li>
                    <li gridster-item="widget" ng-repeat="(widgetIndex,widget) in customReport.widgets"><!--ng-if="widget.pageNumber==pages"-->
                        <div ng-if="widget.widgetType=='reportHeadingWidget'||widget.widgetType=='reportParaWidget'" style="height: 100%;width: 100%;">
                            <div class="box getBox-{{widget.id}}" style=" border: 0px !important;"  id="{{widget.id}}" >
                                <div class="col col-sm-12 col-md-12 col-lg-12 reportbox-header" id="getWidgetColor-{{widget.id}}"  style="border-bottom:0px !important;background-color: #ffffff;"> <!--ng-style="{'background-color': customReport.widgetData[widget.id].color}"-->
                                    <div class='col-sm-2 col-md-2 col-lg-2 center' style="float: left;padding: 5px;">

                                    </div>
                                    <div class='col-sm-7 col-md-8 col-lg-8 center' style="float: left;padding: 0px;">
                                        <h3 ng-style="widget.sizeX!=1  && {'width':'80%'}  || widget.sizeX==1 && {'width':'50%'} ">{{widget.name}}</h3>
                                    </div>

                                    <div class="col-sm-3 col-md-2 col-lg-2 reportGraph-icon" style="float: right;text-align:right">
                                        <a title="Push up this widget" ng-hide="widgetIndex==0"  ng-click="pushUpWidget(widget,widgetIndex)"><i class="fa fa-hand-o-up" aria-hidden="true"></i></a><!--ng-hide="widgetIndex==0||widget.sizeY>reportPagesDetails[widget.pageNumber-1].sizeLeft||(reportPagesDetails[widget.pageNumber-1].lastWigdetIndex!=undefined && widget.sizeY!=customReport.widgets[reportPagesDetails[widget.pageNumber-1].lastWigdetIndex].sizeY)"-->
                                        <a title="Pull down this widget" ng-hide="widgetIndex==customReport.widgets.length-2"  ng-click="pullDownWidget(widget,widgetIndex)"><i class="fa fa-hand-o-down" aria-hidden="true"></i></a><!--ng-hide="widgetIndex==customReport.widgets.length-2||widget.sizeY>reportPagesDetails[widget.pageNumber+1].sizeLeft||(reportPagesDetails[widget.pageNumber+1].firstWigdetIndex!=undefined&&widget.sizeY!=customReport.widgets[reportPagesDetails[widget.pageNumber+1].firstWigdetIndex].sizeY)" -->
                                        <a title="Edit text content" ng-show='widget.textAreaHide==true' ng-click="disableTextEdit(widget,false)"><i class="fa fa-pencil-square-o"></i></a>
                                        <a title="Save text content" ng-hide='widget.textAreaHide==true' ng-click="disableTextEdit(widget,true)"><i class="fa fa-floppy-o"></i></a>
                                        <a title="Remove from Report" ng-click="deleteWidgetFromReport(widget)"><i class="fa fa-trash-o" aria-hidden="true"></i></a>
                                    </div>
                                </div>
                                <div class="box-content widgetBoxBorder" style="top:25px!important;padding: 0px !important;vertical-align: middle" ng-style="{position:(isMobile?'static':'absolute')}">
                                    <div class="row getWidgetGraph textWidget" style="height: 100%;margin:0px;" ng-if="widget.widgetType=='reportParaWidget'">
                                        <!--<textarea class="para-widget" ng-hide='widget.textAreaHide==true' placeholder="Enter your text here" ng-model="widget.textData" text-size-watcher text-size-watcher-widget='widget' text-size-watcher-data="widget.textData" report-page-copy="reportPagesDetails[widget.pageNumber]" report-widgets-copy="customReport.widgets" maxlength="500"></textarea>-->
                                        <textarea class="para-widget" ng-hide='widget.textAreaHide==true' placeholder="Enter your text here" ng-model="widget.textData"  maxlength="500"></textarea>
                                        <p class="para-widget" ng-show='widget.textAreaHide==true'>{{widget.textData}}</p>
                                    </div>
                                    <div class="row getWidgetGraph textWidget" style="height: 100%;margin:0px;" ng-if="widget.widgetType=='reportHeadingWidget'">
                                        <!--<textarea class="head-widget" ng-hide='widget.textAreaHide==true' placeholder="Enter your text here" ng-model="widget.textData" text-size-watcher text-size-watcher-widget='widget' text-size-watcher-data="widget.textData" report-page-copy="reportPagesDetails[widget.pageNumber]" report-widgets-copy="customReport.widgets"  maxlength="75"></textarea>-->
                                        <textarea class="head-widget" ng-hide='widget.textAreaHide==true' placeholder="Enter your text here" ng-model="widget.textData"  maxlength="75"></textarea>
                                        <p class="head-widget" ng-show='widget.textAreaHide==true'>{{widget.textData}}</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div ng-if="widget.widgetType=='pageBreakWidget'"  style="height: 100%;width: 100%;">
                            <div class="box darkblue-bg" style=" border: 0px !important;"  >
                                <div class='pagebreak-header'> <!--ng-style="{'background-color': customReport.widgetData[widget.id].color}"-->
                                    <div style="text-align:center;padding:5px 0px">
                                        <h3>- - - - - &nbsp;Page Break&nbsp; - - - - -</h3>
                                    </div>
                                    <!--<div class="graph-icon">-->
                                    <!--<a title="Push up this widget" ng-click="pushUpWidget(widget,widgetIndex)"><i class="fa fa-hand-o-up" aria-hidden="true"></i></a>-->
                                    <!--<a title="Pull down this widget" ng-click="pullDownWidget(widget,widgetIndex)"><i class="fa fa-hand-o-down" aria-hidden="true"></i></a>-->
                                    <!--</div>-->
                                </div>
                                <div class="box-content " style="top:30px !important;padding: 10px !important;vertical-align: middle" ng-style="{position:(isMobile?'static':'absolute')}">
                                    <div class="row" style="height: 100%;margin:0px;" >
                                        <div class="col col-sm-12 col-md-12 col-lg-12" style="margin-top:2px">
                                            <div class="col-sm-12 col-md-12 col-lg-12 text-center">
                                                <button class="orange rightMargin add-text"  ng-click="addTextWidget('reportHeadingWidget',widget.pageNumber+1,widgetIndex+1,widget.row+1)" ><!-- ng-class="{'exportDisabled': loadedWidgetCount != customReport.widgets.length}" ng-disabled="loadedWidgetCount != customReport.widgets.length" -->
                                                    <i class="fa fa-header" aria-hidden="true"></i>&nbsp;Add Title
                                                </button>
                                                <button class="orange rightMargin add-text"  ng-click="addTextWidget('reportParaWidget',widget.pageNumber+1,widgetIndex+1,widget.row+1)"><!-- ng-class="{'exportDisabled': loadedWidgetCount != customReport.widgets.length}" ng-disabled="loadedWidgetCount != customReport.widgets.length" -->
                                                    <i class="fa fa-paragraph" aria-hidden="true"></i>&nbsp;Add Text
                                                </button>
                                            </div >
                                            <!--<div class="col-sm-6 col-md-6 col-lg-6 text-left">-->
                                            <!---->
                                            <!--&lt;!&ndash;<div style='width:100%' class="daterange daterange&#45;&#45;double three"></div>&ndash;&gt;-->
                                            <!--</div>-->

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div ng-if="widget.widgetType!='reportHeadingWidget'&&widget.widgetType!='reportParaWidget'&&widget.widgetType!='pageBreakWidget'" style="width: 100%;height: 100%">
                            <div class="box widgetBoxBorder getBox-{{widget.id}}"   id="{{widget.id}}">
                                <div class="reportbox-header col col-sm-12 col-md-12 col-lg-12" id="getWidgetColor-{{widget.id}}" ref="{{customReport.widgetData[widget.id].color}}" style="border-bottom: 1px solid #efefef !important;background-color: #ffffff;"> <!--ng-style="{'background-color': customReport.widgetData[widget.id].color}"-->
                                    <div class="col-sm-2 col-md-2 col-lg-1" style="float: left;text-align: right;">
                                        <img src="image/thumbnail/{{widget.channelName}}.png" style="vertical-align: super;max-height: 25px; max-width: 50px; padding:5px 0px 0px 5px"/>
                                        <!--<h3 ng-style="widget.sizeX!=1  && {'width':'80%'}  || widget.sizeX==1 && {'width':'50%'} ">{{customReport.widgetData[widget.id].name}}</h3>-->
                                    </div>
                                    <div class="col-sm-7 col-md-8 col-lg-9" style="float: left;text-align: left;padding: 0px;overflow:hidden">
                                        <!--<img src="image/thumbnail/{{widget.channelName}}.png" style="vertical-align: super;max-height: 31px; max-width: 50px; padding:5px"/>-->
                                        <h3 ng-style="widget.sizeX!=1  && {'width':'80%'}  || widget.sizeX==1 && {'width':'50%'} ">{{customReport.widgetData[widget.id].name}}</h3>
                                    </div>

                                    <div class="col-sm-3 col-md-2 col-lg-2 reportGraph-icon" style="float: right;text-align:right">
                                        <a title="Push up this widget" ng-hide="widgetIndex==0"  ng-click="pushUpWidget(widget,widgetIndex)"><i class="fa fa-hand-o-up" aria-hidden="true"></i></a><!--ng-hide="widgetIndex==0||widget.sizeY>reportPagesDetails[widget.pageNumber-1].sizeLeft||(reportPagesDetails[widget.pageNumber-1].lastWigdetIndex!=undefined && widget.sizeY!=customReport.widgets[reportPagesDetails[widget.pageNumber-1].lastWigdetIndex].sizeY)"-->
                                        <a title="Pull down this widget" ng-hide="widgetIndex==customReport.widgets.length-2"  ng-click="pullDownWidget(widget,widgetIndex)"><i class="fa fa-hand-o-down" aria-hidden="true"></i></a><!--ng-hide="widgetIndex==customReport.widgets.length-2||widget.sizeY>reportPagesDetails[widget.pageNumber+1].sizeLeft||(reportPagesDetails[widget.pageNumber+1].firstWigdetIndex!=undefined&&widget.sizeY!=customReport.widgets[reportPagesDetails[widget.pageNumber+1].firstWigdetIndex].sizeY)" -->
                                        <a title="Remove from Report" ng-click="deleteWidgetFromReport(widget)"><i class="fa fa-trash-o" aria-hidden="true"></i></a>
                                    </div>
                                </div>

                                <div class="box-content" style="vertical-align: middle" ng-style="{position:(isMobile?'static':'absolute')}">
                                    <div ng-if="customReport.widgetData[widget.id].visibility == false" style="height:100%;">
                                        <div class="spiner-example" id="widgetData-{{widget.id}}">
                                            <div class="sk-spinner sk-spinner-double-bounce">
                                                <div class="sk-double-bounce1"></div>
                                                <div class="sk-double-bounce2"></div>
                                            </div>
                                        </div>
                                        <div class="errorWidgetData" id="errorWidgetData-{{widget.id}}">
                                            <div class="row" style="margin: 0px;height: 100%">
                                                <div style="height:99%;">
                                                    <div style="position:relative; top:36%; text-align:center; padding-right:20px">
                                                        <p style="word-wrap:break-word;font-size: 10px;">Your credential is insufficient to fetch data for this metric</p>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class=" errorWidgetData  background-color" id="errorWidgetTokenexpire-{{widget.id}}" style="margin-top: 0%;">
                                            <div class="row" style="margin: 0px;height: 100%">
                                                <div style="height:99%;">
                                                    <div style="position:relative; top:25%; text-align:center; padding-right:20px">
                                                        <p style="word-wrap:break-word;font-size: 10px;">Access to one or more profiles has expired</p>
                                                        <a ><i style="color: #23527c;" class="fa fa-refresh" aria-hidden="true"> </i><p style="color:#000;font-size: 10px">  Refresh</p></a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row getWidgetGraph" style="height: 100%;margin:0px;" ng-if="customReport.widgetData[widget.id].visibility == true">
                                        <div ng-repeat="(chartIndex,chart) in customReport.widgetData[widget.id].chart" ng-if="chart.options.chart.type=== 'lineChart' || chart.options.chart.type=== 'engagedUsersReach' || chart.options.chart.type=== 'area' ||chart.options.chart.type=== 'pieChart' || chart.options.chart.type=== 'reachVsImpressions' || chart.options.chart.type=== 'multiBarChart' || chart.options.chart.type=== 'multiChart'" style="float: left;padding-left: 2px;padding-right: 2px;" ng-style="{'height': (100/customReport.widgetData[widget.id].layoutOptionsY)+'%', 'width': (100/customReport.widgetData[widget.id].layoutOptionsX)+'%'}">
                                            <div class="col col-lg-4 col-sm-4 col-md-4 scrollbar scroller" ng-if="customReport.widgetData[widget.id].showSummary !=false" style="margin:0px;height:100%;padding-left:5px;padding-right:5px;"><!--  ng-style="calculateSummaryHeight(widget.sizeY,chart.data.length)">-->
                                                <ul style="height:100%;font-size:12px;padding-left: 5px;">
                                                    <li  ng-repeat="data in chart.data" ng-class="{'summaryAlign': chart.data.length<=3}" style="text-align: center;padding-top: 1px;padding-left:0px;padding-right: 2px;font-size: 1.0em!important;" id="li-{{widget.id}}-{{$index}}" class="col col-lg-12" ng-style="calculateRowHeight(data,chart.data.length,widget.sizeY,customReport.widgetData[widget.id].layoutOptionsX)"><!--size-watcher-width="data.mywidth"--><!--ng-class="calculateColumnWidth(chart.data.length,widget.sizeX,customReport.widgetData[widget.id].layoutOptionsX)" ng-style="calculateRowHeight(data,chart.data.length,widget.sizeX,widget.sizeY,customReport.widgetData[widget.id].layoutOptionsX)"-->
                                                        <div style="margin: 0px;height: 100%;overflow: hidden;white-space: nowrap;color: #000000;text-align: center;" >
                                                            <p class="summaryKeyFont" ng-class="{'reportSummaryKeyDoubleFont': customReport.widgetData[widget.id].layoutOptionsX>1}" ng-if="chart.options.chart.type != 'pieChart'" ng-show="data.showComparision==false" ng-style="{ 'font-family' : 'Montserrat-Regular','text-transform':'uppercase',  'border-bottom': '4px solid', 'border-color': data.color, 'padding-left': '3px'}" style="margin:0px 0px 3px!important;font-weight: bold;padding-top: 2px;padding-bottom: 2px;"> {{data.key}} <span ng-if="widget.widgetType=='customFusion'">{{' ('+data.channelName+')'}}</span> - {{data.summaryDisplay}} <i ng-if="data.arrow==='up' && data.variance !=0" style="font-size:1.0em;color:#2A8539" class="fa fa-arrow-up" aria-hidden="true"></i><i ng-if='data.arrow==="down" && data.variance !=0' style="font-size: 1.0em;color:#FF1B1B" class="fa fa-arrow-down" aria-hidden="true"></i></p>
                                                            <p class="summaryKeyFont" ng-class="{'reportSummaryKeyDoubleFont': customReport.widgetData[widget.id].layoutOptionsX>1}" ng-if="chart.options.chart.type == 'pieChart'" ng-show="data.showComparision==false" ng-style="{ 'font-family' : 'Montserrat-Regular', 'text-transform':'uppercase',  'border-bottom': '4px solid', 'border-color': data.color, 'padding-left': '3px'}" style="margin:0px 0px 3px!important;font-weight: bold;padding-top: 2px;padding-bottom: 2px;"> {{data.key}} <span ng-if="widget.widgetType=='customFusion'">{{' ('+data.channelName+')'}}</span>- {{data.summaryDisplay}}</p>
                                                            <p class="summaryKeyFont" ng-class="{'reportSummaryKeyDoubleFont': customReport.widgetData[widget.id].layoutOptionsX>1}" ng-show="data.showComparision==true" ng-style="{ 'font-family' : 'Montserrat-Black', 'text-transform':'uppercase',  'border-bottom': '4px solid', 'border-color': data.color, 'padding-left': '3px'}" style="margin:0px 0px 3px!important;font-weight: bold;padding-top: 2px;padding-bottom: 2px;"> {{data.key}}<span ng-if="widget.widgetType=='customFusion'">{{' ('+data.channelName+')'}}</span></p>
                                                            <div class="col col-lg-12 col-sm-12 col-md-12 col-xs-12 " style="padding: 0px 0px 0px 0px!important;">
                                                                <div class="col col-lg-5 col-sm-5 col-md-5 col-xs-5">
