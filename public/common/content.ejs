<!-- ModalContents -->
<div class="md-modal md-effect-19" id="exportModalContent">
    <div class="md-content  pleaseWait" ng-controller="ExportController">
        <div class="wrapper wrapper-content">
            <div class="row">
                <div class="col-md-12">
                    <div class="center">
                        <button type="button" class="close exportCloseButton closeModalContent" aria-hidden="true">Ã—</button>
                        <h5 class="exportHeadText">Choose your Export option   </h5>
                        <hr/><br/>
                        <label class="btn btn-default"><input type="radio" id="exportOptionJpeg" name="exportOption" value="JPEG"/> JPEG</label>
                        <label class="btn btn-default"><input type="radio" id="exportOptionPDF" name="exportOption" value="PDF"/> PDF</label>
                        <label class="btn btn-default"><input type="radio" id="exportOptionUrl" name="exportOption" value="url"/> Share as HTML</label>
                    </div>
                </div>
            </div>
            <br/>
            <div class="row">
                <div class="col-md-12">
                    <div class="center">
                        <span class="errorExportMessage"></span>
                    </div>
                </div>
            </div>
            <br/>
            <div class="row">
                <div class="col-md-12">
                    <div class="pull-right">
                        <button class="btn btn-primary" id="submitExportButton" ng-click="submitExport();closeExport();">Export</button>
                    </div>
                </div>
            </div>

            <div class="scrollbar" id="scroller">
                <div class="row">
                    <h5 class="model-title">PDF Print Preview</h5>
                    <div class="col-lg-12">
                        <div id="dashLayout" class="dashboardLayout">
                            <div class="white-bg dashboardContent" style="border: 0px solid;" ng-repeat="aexppage in expPages">
                                <div class="border-bottom dashboardTitle white-bg" style="max-height:40px">
                                    <div class="row">
                                        <div class="col col-sm-12">
                                            <div class="dashboardTemplate-items">
                                                <div class="col-sm-3 dashboardName">
                                                    <p ng-style="!expCtrl.dashboard.dashboardName  && {'color':'#999'}  || expCtrl.dashboard.dashboardName && {'color':'#000'}">{{expCtrl.dashboard.dashboardName || ''}}</p>
                                                </div>
                                                <div class="col-sm-4">
                                                    <div class="daterange daterange--double three"></div>
                                                </div>
                                                <div class="col-sm-3">
                                                    <div>Page: {{$index + 1}}</div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div gridster="expgridsterOptions" class="row" style="border: 1px solid lightgrey;min-height:500px">
                                    <ul>
                                        <li gridster-item="widget" ng-repeat="(widgetIndex,widget) in aexppage.widgets">
                                            <div class="box getBox-{{widget.id}}" ng-controller="CustomWidgetController" id="{{widget.id}}">
                                                <div class="box-header" id="getWidgetColor-{{widget.id}}" ref="{{aexppage.widgetData[widgetIndex].color}}" ng-style="{'background-color': aexppage.widgetData[widgetIndex].color}">
                                                    <h3 style="color: #ffffff;">{{aexppage.widgetData[widgetIndex].name}}</h3>
                                                </div>
                                                <div class="box-content" style="vertical-align: middle" ng-style="{position:(isMobile?'static':'absolute')}">
                                                    <div class="row getWidgetGraph" style="height: 100%;margin:0px;" ng-if="aexppage.widgetData[widgetIndex].visibility == true">
                                                        <div ng-repeat="chart in aexppage.widgetData[widgetIndex].chart track by $index" ng-if="chart.options.chart.type=== 'lineChart'  || chart.options.chart.type=== 'pieChart' || chart.options.chart.type=== 'multiBarChart' " style="float: left;padding-left: 2px;padding-right: 2px;" ng-style="{'height': (100/aexppage.widgetData[widgetIndex].layoutOptionsY)+'%', 'width': (100/aexppage.widgetData[widgetIndex].layoutOptionsX)+'%'}">
                                                            <div class="row" style="height: 20%">
                                                                <ul style="height:100%;font-size:14px; padding-left: 5px;padding-right: 5px;">
                                                                    <li ng-repeat="data in chart.data track by $index"
                                                                        style="float:left;text-align:center;padding-top: 5px;padding-left:20px;padding-right: 5px;">
                                                                        <div style="margin: 0px;height: 100%;overflow: hidden;white-space: nowrap;color: #000000;text-align: center">
                                                                            <p ng-style="{'color': '#000000', 'font-family' : 'Bebas Neue'}" style="font-weight: bold;">{{data.key}} - <b ng-style="{'color':data.color, 'font-family' : 'Bebas Neue'}">{{data.summaryDisplay}} </b></p>
                                                                        </div>
                                                                    </li>
                                                                </ul>
                                                            </div>
                                                            <div class="row" style="margin: 0px;height: 80%">
                                                                <nvd3 options="chart.options" data="chart.data" api="chart.api" config="{refreshDataOnly:true, deepWatchDataDepth: 0}" events="events" ng-if="aexppage.widgetData[widgetIndex].visibility == true"></nvd3>
                                                            </div>
                                                        </div>
                                                        <div ng-repeat="chart in aexppage.widgetData[widgetIndex].chart track by $index" ng-if="chart.options.chart.type=== 'instagramPosts'" style="float: left;padding-left: 2px;padding-right: 2px;" ng-style="{'height': (100/aexppage.widgetData[widgetIndex].layoutOptionsY)+'%', 'width': (100/aexppage.widgetData[widgetIndex].layoutOptionsX)+'%'}">
                                                            <div class="row" style="margin: 0px;height: 100%">
                                                                <div events="events" style="height: 99%" ng-if="aexppage.widgetData[widgetIndex].visibility == true">
                                                                    <div class="scrollbar scrollInWidget" id="scroller">
                                                                        <div class="col-lg-12 listed" ng-repeat="data in chart.data track by $index">
                                                                            <div class="col-lg-1 listed-image">
                                                                                <a href="{{data.links}}" target="_blank"><img src="{{data.image}}" height="50px" widget="50px"/></a>
                                                                            </div>
                                                                            <div class="col-lg-11 aside">
                                                                                <div class="postComment"><a class="apiLink" href="{{data.links}} " target="_blank">{{data.postComment}}</a></div>
                                                                                <div class="impression">
                                                                                    <div class="likes"><span>likes:{{data.likes}}</span></div>
                                                                                    <div class="comment"><span>Comments:{{data.comments}}</span></div>
                                                                                    <div class="date"><span>{{data.date}}</span></div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div ng-repeat="chart in aexppage.widgetData[widgetIndex].chart track by $index" ng-if="chart.options.chart.type=== 'highEngagementTweets'" style="float: left;padding-left: 2px;padding-right: 2px;" ng-style="{'height': (100/aexppage.widgetData[widgetIndex].layoutOptionsY)+'%', 'width': (100/aexppage.widgetData[widgetIndex].layoutOptionsX)+'%'}">
                                                            <div class="row" style="margin: 0px;height: 100%">
                                                                <div events="events" style="height: 99%" ng-if="aexppage.widgetData[widgetIndex].visibility == true">
                                                                    <div class="scrollbar scrollInWidget" id="scroller">
                                                                        <div class="col-lg-12 listed" ng-repeat="data in chart.data track by $index" ng-if="data.postComment">
                                                                            <div class="col-lg-12">
                                                                                <div class="postComment"><a class="apiLink" href="{{data.links.media[0].expanded_url}} " target="_blank">{{data.postComment}}</a></div>
                                                                                <div class="impression">
                                                                                    <div class="likes"><span>Likes: {{data.likes}}</span></div>
                                                                                    <div class="comment"><span>Retweets: {{data.reTweet}}</span></div>
                                                                                    <div class="date"><span>{{data.date}}</span></div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </li>
                                        <li gridster-item="widget" ng-repeat="(widgetIndex,widget) in aexppage.empWidget"></li>
                                    </ul>
                                </div>
                                <div style="min-height: 8px;page-break-after: always;" ng-if="$index > 0"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

