<div class="md-modal md-effect-19" id="exportModalContent">
    <div class="md-content pleaseWait" ng-controller="ExportController">

            <div class="wrapper wrapper-content">
                <div class="row">
                    <div class="col-md-12">
                        <div class="center">
                            <h5 class="exportHeadText">Choose your Export option   </h5>
                            <hr/>
                            <br/>
                            <label class="btn btn-default">
                                <input type="radio" id="exportOptionJpeg" name="exportOption" value="JPEG"/> JPEG
                            </label>
                            <label class="btn btn-default">
                                <input type="radio" id="exportOptionPDF" name="exportOption" value="PDF"/> PDF
                            </label>
                        </div>
                    </div>
                </div>
                <br/>
                <div class="row">
                    <div class="col-md-12">
                        <div class="center">
                            <span class="errorExportMessage"></span>
                        </div>
                    </div>
                </div>
                <br/>
                <div class="row">
                    <div class="col-md-12">
                        <div class="pull-right">
                            <button class="btn btn-primary" id="submitExportButton" ng-click="submitExport();closeExport();">Export</button>
                        </div>
                    </div>
                </div>
            </div>

    </div>
</div>

